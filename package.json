  "author": "Gianni Dalerta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Purple-Horizons/openclaw-porter.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "js-yaml": "^4.1.0",
    "tar": "^7.0.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
