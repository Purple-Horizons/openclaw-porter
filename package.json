{
  "name": "@purple-horizons/openclaw-porter",
  "version": "0.1.0",
  "description": "Export and import OpenClaw/Clawdbot agents with full context",
  "main": "dist/index.js",
  "bin": {
    "openclaw-porter": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
